// prisma/schema.prisma
// Define your database models

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}



model Contact {
  id        Int      @id @default(autoincrement())
  name      String                           
  phone     String                          
  email     String?                      
  address   String?                      
  createdAt DateTime @default(now())         
  updatedAt DateTime @updatedAt           
}
